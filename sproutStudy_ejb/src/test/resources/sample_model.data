    "8032D17C-B2FE-4B49-BAD7-B63F70C2B660": {
      "en": "Audiogram/hearing test.",
      "es": "Audiograma/prueba para la audición.\r"
    },
    "C226AE3C-EABB-4EB5-8F0C-611756D535F6": {
      "en": "The AAP recommends that all children with Down syndrome receive an audiogram every 6 months until hearing is confirmed in both ears, and then annually thereafter until age 21, given increased incidence of hearing loss in this population.",
      "es": "La Academia Americana de Pediatría recomienda que los niños con síndrome de Down se realicen un audiograma cada 6 meses hasta que se haya confirmado la audición en ambos oídos. A partir de entonces, y dada la incidencia de pérdida de la audición en esta población, un audiograma al año hasta la edad de 21 años.\r"
    },
    "421D49E9-6651-4922-8612-B7445C96760C": {
      "en": "Audiogram/hearing test.",
      "es": "Audiograma/prueba para la audición.\r"
    },
    "CDB75D9B-0FE6-4BD5-8CB0-9433E32F5C61": {
      "en": "For ages 1 to 21, AAP recommends annual audiograms given increased incidence of hearing loss in this population.",
      "es": "La AAP recomienda audiogramas entre la edad de 1 y 21 años, dada la creciente incidencia de pérdida de la audición en esta población.\r"
    },
    "51A50AE8-EF27-4DF5-9AD2-D62098239B12": {
      "en": "Audiogram/hearing test.",
      "es": "Audiograma/prueba para la audición.\r"
    },
    "90AAD713-421B-4500-8F3A-C7F4A4ED584A": {
      "en": "For ages 21 and over, experts recommend an audiogram every two years, given increased incidence of hearing loss in this population.",
      "es": "Para las personas de 21 años en adelante, los expertos recomiendan un audiograma cada dos años, dada la creciente incidencia de pérdida de la audición en esta población.\r"
    },
    "A88764D6-D9DA-41B3-B819-A64A03270A58": {
      "en": "Audiogram/hearing test.",
