    "F9F8AAA0-19B9-458D-BD8B-972BA5FE49BC": {
      "en": "Thank you very much for taking the time to complete the Down Syndrome Clinic to You (DSC2U) intake form.",
      "es": "Muchas gracias por tomarse el tiempo para completar el formulario de admisión a la Clínica de síndrome de Down para usted (DSC2U).\r"
    },
    "DA66D886-8423-4FB2-A5F6-32D70868A82D": {
      "en": "We used your responses to create this personalized checklist of information, resources, and recommendations for {{dsc2ujeanhee.first_name}}.",
      "es": "Utilizamos sus respuestas para crear esta lista de referencia personalizada con información, recursos y recomendaciones para {{dsc2ujeanhee.first_name}}.\r"
    },
    "30ACC1EA-CAD4-4D5F-981A-A58C08016F93": {
      "en": "To find out why certain suggestions were made, please click on the \"",
      "es": "Para saber por qué se incluyeron ciertas sugerencias, haga clic en el enlace \"\r"
    },
    "ED410C83-A1C1-40A5-B4B2-9F0C00D266B3": {
