    },
    "3BC56A12-6464-460A-B5D5-1F6E1A53043B": {
      "en": "banging head",
      "es": "se golpea la cabeza\r"
    },
    "336F8493-2392-4334-BDC6-195910850947": {
      "en": "hitting or kicking others",
      "es": "golpea o patea a otros\r"
    },
    "72E584E1-EC7F-4400-850B-51FEA61F584D": {
      "en": "moods changing rapidly for no reason",
      "es": "su humor cambia repentinamente sin ningún motivo\r"
    },
    "269D1AAA-2600-449B-9DF3-53B41EE65CD0": {
      "en": "aggression",
      "es": "es agresivo\r"
    },
    "C1531201-8372-406C-86C1-EE143C382FEB": {
      "en": "property destruction",
      "es": "destruye cosas\r"
    },
    "47E8B238-C584-4440-B79D-AB92EA1D85EF": {
      "en": "forgetfulness",
      "es": "se muestra olvidadizo\r"
    },
    "09666E71-3BAA-4FEB-9E89-CE43469CA32A": {
      "en": "stereotypic motor movements",
      "es": "realiza movimientos motores estereotípicos\r"
    },
    "237D8F76-AD7D-442D-ACA2-38D447FC6444": {
