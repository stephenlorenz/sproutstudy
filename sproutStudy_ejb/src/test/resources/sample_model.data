      "es": "El cuidador de su paciente,\r"
    },
    "401B4D03-83D0-4C75-BE16-21F9A983C53A": {
      "en": "{{dsc2ujeanhee.first_name}} {{dsc2ujeanhee.last_name}} (DOB {{dsc2ujeanhee.pt_dob}})",
      "es": "{{dsc2ujeanhee.first_name}} {{dsc2ujeanhee.last_name}} (FECHA DE NACIMIENTO {{dsc2ujeanhee.pt_dob}})\r"
